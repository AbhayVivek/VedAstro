@attribute [Route(PageRoute.BirthTimeFinder)]


<PageTitle>Birth Time Finder</PageTitle>
<PageTop Title="Birth Time Finder" IconName="fa-solid:history">
    <Description>
        Find forgotten or lost birth time using astrological methods.
        Not all birth time is recorded accurately at birth, use this tool to find out your correct birth time.
    </Description>
    <ButtonsRow>
        <IconButton IconName="iconoir:donate" ButtonText="Fund" OnClickCallback="() => _navigation.NavigateTo(PageRoute.Donate)"></IconButton>
    </ButtonsRow>
</PageTop>

<h4>Calculator is under development.</h4>
<h4>
    <a href="@PageRoute.Donate" class="link-primary">Fund</a> this calculator for faster development.
</h4>


